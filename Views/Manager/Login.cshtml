<h2>Manager Login</h2>

<form method="post">
    <input name="email" placeholder="Email" required />
    <br /><br />
    <input name="password" type="password" placeholder="Password" required />
    <br /><br />
    <button type="submit">Login</button>
</form>

<p style="color:red">@ViewBag.Error</p>

